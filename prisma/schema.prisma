generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model OffboardingAnalytics {
  id                            String    @id @default(uuid()) @db.Uuid
  source                        String?
  period                        String?
  offboarding_report_request_id String
  offboarding_report_json       Json
  workflow_report_request_id    String
  workflow_report_json          Json
  data                          Json
  fetchedAt                     DateTime  @default(now())
  updateAt                      DateTime  @updatedAt
}


